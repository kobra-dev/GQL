Query(
  Lambda(
    ["user", "name", "isPublic", "description", "projectJson"],{
      id: ToArray(Create(Collection("projects"), {
        data: {
          user: Var("user"),
          name: Var("name"),
          isPublic: Var("isPublic"),
          description: Var("description"),
          projectJson: Var("projectJson")
        }
      }))[0],
      itemWasModied: true
    }
  )
)